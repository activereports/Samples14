<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2005/01/reportdefinition">
	<Body>
		<Height>5.5in</Height>
		<ReportItems>
			<CustomReportItem Name="Calendar1">
				<Type>Calendar</Type>
				<Width>18.415cm</Width>
				<Height>13.6525cm</Height>
				<Style>
					<Language>en-US</Language>
				</Style>
				<CustomData>
					<DataSetName>DataSet1</DataSetName>
					<DataRowGroupings>
						<DataGroupings>
							<DataGrouping>
								<CustomProperties>
									<CustomProperty>
										<Name>calendar:Event.Value</Name>
										<Value>=Fields!StartTime.Value</Value>
									</CustomProperty>
									<CustomProperty>
										<Name>calendar:Event.EndDate</Name>
										<Value>=Fields!EndTime.Value</Value>
									</CustomProperty>
									<CustomProperty>
										<Name>calendar:Event.StartDate</Name>
										<Value>=Fields!StartTime.Value</Value>
									</CustomProperty>
									<CustomProperty>
										<Name>calendar:Event.Format</Name>
										<Value>=IIF((Convert.ToDateTime(Fields!StartTime.Value).Day - Convert.ToDateTime(Fields!EndTime.Value).Day) = 0, "D", "d")</Value>
									</CustomProperty>
									<CustomProperty>
										<Name>calendar:Event.Backcolor</Name>
										<Value>=IIF((Convert.ToDateTime(Fields!StartTime.Value).Day - Convert.ToDateTime(Fields!EndTime.Value).Day) = 0, "LightGreen", "Coral")</Value>
									</CustomProperty>
								</CustomProperties>
							</DataGrouping>
						</DataGroupings>
					</DataRowGroupings>
				</CustomData>
			</CustomReportItem>
		</ReportItems>
	</Body>
	<Width>7.375in</Width>
	<DataSources>
		<DataSource Name="DataSource1">
			<ConnectionProperties>
				<ConnectString>Path=Calendar-Format.csv;Locale=en-US;TextQualifier=";ColumnsSeparator=,;RowsSeparator=\r\n;Columns=StartTime,EndTime,Value;JoinRowsSeparators=True;HasHeaders=True</ConnectString>
				<DataProvider>CSV</DataProvider>
			</ConnectionProperties>
		</DataSource>
	</DataSources>
	<DataSets>
		<DataSet Name="DataSet1">
			<Fields>
				<Field Name="StartTime">
					<DataField>StartTime</DataField>
				</Field>
				<Field Name="EndTime">
					<DataField>EndTime</DataField>
				</Field>
				<Field Name="Value">
					<DataField>Value</DataField>
				</Field>
			</Fields>
			<Query>
				<CommandText/>
				<DataSourceName>DataSource1</DataSourceName>
			</Query>
		</DataSet>
	</DataSets>
</Report>
