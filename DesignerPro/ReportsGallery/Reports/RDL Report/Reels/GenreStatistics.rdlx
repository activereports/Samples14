					</CustomProperty>
					<CustomProperty>
						<Name>ID</Name>
						<Value>1</Value>
					</CustomProperty>
				</CustomProperties>
				<dd:Container xmlns:dd="http://schemas.datadynamics.com/reporting/2005/02/reportdefinition">
					<ReportItems>
						<Table Name="Table1">
							<DataSetName>GenreStats</DataSetName>
							<Header>
								<TableRows>
									<TableRow>
										<Height>0.25in</Height>
										<TableCells>
											<TableCell>
												<ReportItems>
													<Textbox Name="TextBox3">
														<CanGrow>true</CanGrow>
														<Value>Genre</Value>
